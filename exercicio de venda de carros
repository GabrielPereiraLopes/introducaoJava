
package exercicio1;
import java.util.Scanner;

public class Exercicio1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
      System.out.println("Quantidade de carros vendidos:  ");     
      int carros = sc.nextInt();
      
      System.out.println("Valor total de venda :  ");     
      float valor = sc.nextFloat();
      
      System.out.println("Valor de salario :  ");     
      float salario = sc.nextFloat();
      
      System.out.println("Valor por carro :  ");     
      float preco = sc.nextFloat();
      
      float calculo = salario + (preco * carros) + ((5 * valor)/100);
      
      System.out.println("Salario final: " +calculo);
    }
    
}
